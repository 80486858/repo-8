{
  "name": "@schibsted/account-sdk-browser",
  "version": "3.0.0-rc.3",
  "description": "Schibsted Account SDK for browsers",
  "main": "index.js",
  "scripts": {
    "build": "./build.sh",
    "clean": "rimraf .cache coverage dist docs",
    "docs": "rimraf docs && jsdoc -c ./utils/jsdoc.js --verbose",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "jest",
    "precover": "npm run lint",
    "cover": "jest --coverage",
    "postcover": "codecov"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "fetch-jsonp": "^1.1.3",
    "tiny-emitter": "^2.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "codecov": "^3.0.1",
    "docdash": "git+https://github.com/torarvid/docdash.git#v0.5.0",
    "eslint": "^4.16.0",
    "eslint-plugin-import": "^2.11.0",
    "jest": "^22.1.4",
    "jest-junit": "^3.7.0",
    "jsdoc": "^3.5.5",
    "node-fetch": "^2.1.2",
    "regenerator-runtime": "^0.11.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "whatwg-url": "^6.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/schibsted/account-sdk-browser.git"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "> 1%",
              "last 10 chrome major versions",
              "last 10 firefox major versions",
              "last 10 opera major versions",
              "last 2 safari major versions",
              "last 2 ios major versions",
              "last 2 ie major versions",
              "last 5 edge major versions"
            ]
          }
        }
      ]
    ]
  }
}
